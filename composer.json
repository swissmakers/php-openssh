{
    "name": "swissmakers/php-openssh",
    "description": "Allows you to generate 4096 Bit RSA private/public key-pairs, which can be used as authentication method and to encrypt / decrypt strings.",
    "keywords": [
        "security",
        "php",
        "RSA",
        "openssh",
        "ssh",
        "laravel"
    ],
    "homepage": "https://github.com/swissmakers/php-openssh",
    "license": "GPL-3.0-or-later",
    "authors": [
        {
            "name": "Swissmakers GmbH",
            "email": "info@swissmakers.ch",
            "homepage": "https://www.swissmakers.ch"
        }
    ],
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": "^8.1",
        "illuminate/support": "^8|^9|^10",
        "phpseclib/phpseclib": "^3.0.23"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.4.2",
        "orchestra/testbench": "^8.12.2",
        "phpstan/phpstan": "^1.5",
        "squizlabs/php_codesniffer": "^3.6"
    },
    "autoload": {
        "psr-4": {
            "Swissmakers\\OpenSSH\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Swissmakers\\OpenSSH\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "phpstan": "vendor/bin/phpstan analyse",
        "cs-check": "vendor/bin/phpcs",
        "cs-fix": "vendor/bin/phpcbf"
    }
}
